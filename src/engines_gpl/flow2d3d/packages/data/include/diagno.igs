                             !   1 = BOTT
                             !   2 = CHEM
                             !   3 = ECO
                             !   4 = FLOW
                             !   5 = MOR
                             !   6 = PART
                             !   7 = SED
                             !   8 = TRAN
                             !   9 = WAQ
                             !  10 = WAVE
       integer :: lunsys(mxsys)
                             !  Array containing unit numbers for
                             !  all diagnostic files of Delft3D
                             !  =0 not defined and/or not opened
!
    end type gd_diagno
